{
  "version": 1,
  "projects": {
    "transloco": {
      "projectType": "library",
      "root": "libs/transloco",
      "sourceRoot": "libs/transloco/src",
      "prefix": "transloco",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/transloco/tsconfig.lib.json",
            "project": "libs/transloco/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/transloco/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/transloco/tsconfig.lib.json", "libs/transloco/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/transloco/**"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/transloco/src/test.ts",
            "tsConfig": "libs/transloco/tsconfig.spec.json",
            "karmaConfig": "libs/transloco/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
