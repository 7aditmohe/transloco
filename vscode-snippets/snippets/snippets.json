{
  "StructuralDirective": {
    "body": ["<ng-template transloco let-t>", "  {{ t('${1:key}') }}", "</ng-template>"],
    "prefix": "trd",
    "description": "Create a transloco structural directive"
  },
  "NgContainer": {
    "prefix": "trc",
    "body": ["<ng-container *transloco=\"let t\">", "  {{ t('${1:key}') }}", "</ng-container>"],
    "description": ""
  },
  "Directive": {
    "body": ["<span transloco=\"title\" [translocoParams]=\"{ value: 'value' }\"></span>"],
    "prefix": "trdd",
    "description": "Transloco directive"
  },
  "Pipe": { "body": ["{{ '${1:key}' | transloco }}"], "prefix": "trp", "description": "Transloco Pipe" }
}
